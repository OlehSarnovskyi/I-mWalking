{
  "name": "im-walking",
  "version": "1.0.0",
  "description": "Full-stack app",
  "main": "backend/index.js",
  "engines": {
    "node": "12.16.3",
    "npm": "6.14.10"
  },
  "scripts": {
    "backend_start": "nodemon backend/index.js",
    "backend_install": "npm install",
    "frontend_install": "npm install --prefix frontend",
    "frontend_start:ImWalking": "npm run start --prefix frontend --project ImWalking",
    "frontend_start:host:ImWalking": "npm run start:host --prefix frontend --project ImWalking",
    "frontend_start:hmr:ImWalking": "npm run start:hmr --prefix frontend --project ImWalking",
    "frontend_build:ImWalking": "npm run build --prefix frontend --project ImWalking",
    "heroku_postbuild": "NPM_CONFIG_PRODUCTION=false npm run frontend_install && npm run frontend_build:ImWalking",
    "postinstall": "npm run frontend_build:ImWalking"
  },
  "author": "Oleh Sarnovskyi",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.9",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
